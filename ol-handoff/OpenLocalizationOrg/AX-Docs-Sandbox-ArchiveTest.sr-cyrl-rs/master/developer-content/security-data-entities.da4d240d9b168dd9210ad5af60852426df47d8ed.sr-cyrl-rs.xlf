          <source>Authorize</source>
        </trans-unit>
        <trans-unit id="208" translate="yes" xml:space="preserve">
          <source>…IntegrationApprove (Release, Confirm, Journalize)</source>
        </trans-unit>
        <trans-unit id="209" translate="yes" xml:space="preserve">
          <source>Approve (Release, Confirm, Journalize) …</source>
        </trans-unit>
