        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>A mobile workspace must be published so that users can access it in the Dynamics 365 for Unified Operations mobile app.</source>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The mobile app was previously named <bpt id="p1">*</bpt>Microsoft Dynamics 365 for Finance and Operations<ept id="p1">*</ept>.</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Publish a mobile workspace by using Microsoft Dynamics 365 for Finance and Operations, Enterprise edition July 2017 update</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>In your browser, start Microsoft Dynamics 365 for Finance and Operations, Enterprise edition.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Settings<ept id="p1">**</ept><ph id="ph1"> &gt; </ph><bpt id="p2">**</bpt>Mobile app<ept id="p2">**</ept>.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>Select the mobile workspace to publish.</source>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Publish<ept id="p1">**</ept>.</source>
        </trans-unit>
