        <trans-unit id="123" translate="yes" xml:space="preserve">
          <source>You can then do a manual update in the <bpt id="p1">**</bpt>Cost management<ept id="p1">**</ept> or <bpt id="p2">**</bpt>Cost analysis<ept id="p2">**</ept> workspace.</source>
