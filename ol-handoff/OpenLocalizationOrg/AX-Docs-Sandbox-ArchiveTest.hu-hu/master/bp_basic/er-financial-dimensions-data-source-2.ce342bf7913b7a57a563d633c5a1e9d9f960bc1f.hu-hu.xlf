        <trans-unit id="184" translate="yes" xml:space="preserve">
          <source>In the tree, select 'LedgerJournal<ph id="ph1">\&lt;</ph>Relations\LedgerJournalTrans\Credit(AmountCurCredit)'.</source>
        </trans-unit>
        <trans-unit id="185" translate="yes" xml:space="preserve">
