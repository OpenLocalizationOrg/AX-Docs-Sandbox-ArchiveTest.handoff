        <trans-unit id="187" translate="yes" xml:space="preserve">
          <source>In the tree, select 'LedgerJournal<ph id="ph1">\&lt;</ph>Relations\LedgerJournalTrans'.</source>
        </trans-unit>
        <trans-unit id="188" translate="yes" xml:space="preserve">
