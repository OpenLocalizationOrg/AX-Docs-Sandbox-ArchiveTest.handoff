        <trans-unit id="244" translate="yes" xml:space="preserve">
          <source>She verifies the following information:</source>
        </trans-unit>
        <trans-unit id="245" translate="yes" xml:space="preserve">
          <source>For items that require three-way matching, the quantity on the invoice line matches the quantity that was received.</source>
        </trans-unit>
        <trans-unit id="246" translate="yes" xml:space="preserve">
