        </trans-unit>
        <trans-unit id="296" translate="yes" xml:space="preserve">
          <source>IF( 'Route transactions'[Total mixed quantity] = 0, BLANK(), (SUM('Route transactions'[Defective quantity]) / 'Route transactions'[Total mixed quantity]) <ph id="ph1">\*</ph> 1000000)</source>
        </trans-unit>
