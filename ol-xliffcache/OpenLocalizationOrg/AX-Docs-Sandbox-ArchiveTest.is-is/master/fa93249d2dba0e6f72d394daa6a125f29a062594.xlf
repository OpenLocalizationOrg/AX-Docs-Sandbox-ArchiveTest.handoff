        <trans-unit id="245" translate="yes" xml:space="preserve">
          <source>COUNTROWS('Planned production order')</source>
        </trans-unit>
        <trans-unit id="246" translate="yes" xml:space="preserve">
          <source>Early</source>
        </trans-unit>
        <trans-unit id="247" translate="yes" xml:space="preserve">
          <source>COUNTROWS(FILTER('Planned production order', 'Planned production order'[Scheduled end date] <ph id="ph1">\&lt;</ph> 'Planned production order'[Requirement date]))</source>
