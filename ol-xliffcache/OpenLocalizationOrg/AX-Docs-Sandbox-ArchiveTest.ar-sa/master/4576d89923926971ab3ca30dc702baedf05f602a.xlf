        <trans-unit id="177" translate="yes" xml:space="preserve">
          <source>Go to Organization administration &gt; Electronic reporting &gt; Configurations.</source>
        </trans-unit>
        <trans-unit id="178" translate="yes" xml:space="preserve">
          <source>In the tree, expand 'Model for cheques'.</source>
        </trans-unit>
        <trans-unit id="179" translate="yes" xml:space="preserve">
          <source>In the tree, select 'Model for cheques\Cheques printing format'.</source>
        </trans-unit>
        <trans-unit id="180" translate="yes" xml:space="preserve">
          <source>Click Designer.</source>
        </trans-unit>
        <trans-unit id="181" translate="yes" xml:space="preserve">
          <source>Click Attachments.</source>
        </trans-unit>
        <trans-unit id="182" translate="yes" xml:space="preserve">
          <source>Click Delete.</source>
