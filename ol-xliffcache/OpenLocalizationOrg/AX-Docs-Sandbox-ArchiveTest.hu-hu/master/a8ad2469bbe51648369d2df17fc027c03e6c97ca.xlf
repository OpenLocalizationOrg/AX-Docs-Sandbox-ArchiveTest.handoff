          <source>Select a route</source>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Go to Transportation management &gt; Planning &gt; Load planning workbench.</source>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Clear the Hide shipped and received check box.</source>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>Select the line with Load ID 00006.</source>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
