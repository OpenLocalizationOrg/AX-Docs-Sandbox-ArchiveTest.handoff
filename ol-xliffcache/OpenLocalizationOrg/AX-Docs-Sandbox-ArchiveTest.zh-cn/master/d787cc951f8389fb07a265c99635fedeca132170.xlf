        <trans-unit id="162" translate="yes" xml:space="preserve">
          <source>In the Formula field, enter 'IF(Intrastat.CommodityRecord.Direction=Direction.Import, "Import", "Export")'.</source>
        </trans-unit>
        <trans-unit id="163" translate="yes" xml:space="preserve">
